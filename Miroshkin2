import re
import os

from UDIN import parse_readme_001
from SHABANOV import parse_readme_002
from Miroshkin import parse_readme_003
from Malinevskiy import parse_readme_004

def main():
    while True:
        print("\n" + "="*50)
        a = input("–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä README —Ñ–∞–π–ª–∞ (1-4) –∏–ª–∏ 'new' –¥–ª—è –≤–≤–æ–¥–∞ –Ω–æ–≤—ã—Ö –¥–∞–Ω–Ω—ã—Ö: ")
        print("–ß—Ç–æ–±—ã –∑–∞–≤–µ—Ä—à–∏—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É –Ω–∞–ø–∏—à–∏—Ç–µ 'end'")
        
        if a.lower() == "end":
            break
        elif a.lower() == "new":
            create_readme_new1()
            output("new")
        else:
            output(a)

def create_readme_new1():
    """–§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è —Ñ–∞–π–ª–∞ READMENEW.md —Å –¥–∞–Ω–Ω—ã–º–∏ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è"""
    print("\n" + "="*50)
    print("üìù –ó–∞–ø–æ–ª–Ω–∏—Ç–µ –¥–∞–Ω–Ω—ã–µ –¥–ª—è —Ñ–∞–π–ª–∞ READMENEW.md:")
    print("="*50)
    
    # –ó–∞–ø—Ä–∞—à–∏–≤–∞–µ–º –¥–∞–Ω–Ω—ã–µ —É –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è
    college = input("–£—á–µ–±–Ω–æ–µ –∑–∞–≤–µ–¥–µ–Ω–∏–µ: ")
    course = input("–ö—É—Ä—Å: ")
    name = input("–§–ò–û: ")
    group = input("–ö–æ–º–∞–Ω–¥–∞/–ì—Ä—É–ø–ø–∞: ")
    id = input("ID: ")
    
    # –°–æ–∑–¥–∞–µ–º —Å–æ–¥–µ—Ä–∂–∏–º–æ–µ —Ñ–∞–π–ª–∞ –≤ —Ñ–æ—Ä–º–∞—Ç–µ –∫–ª—é—á: –∑–Ω–∞—á–µ–Ω–∏–µ
    content = f"""# –ù–æ–≤—ã–π –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å

## –î–∞–Ω–Ω—ã–µ —Å—Ç—É–¥–µ–Ω—Ç–∞
–£—á–µ–±–Ω–æ–µ –∑–∞–≤–µ–¥–µ–Ω–∏–µ: {college}
–ö—É—Ä—Å: {course}
–§–ò–û: {name}
–ö–æ–º–∞–Ω–¥–∞: {group}
ID: {id}

## –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è
–≠—Ç–æ—Ç —Ñ–∞–π–ª –±—ã–ª —Å–æ–∑–¥–∞–Ω –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —á–µ—Ä–µ–∑ –ø—Ä–æ–≥—Ä–∞–º–º—É.
"""
    
    # –ó–∞–ø–∏—Å—ã–≤–∞–µ–º –¥–∞–Ω–Ω—ã–µ –≤ —Ñ–∞–π–ª READMENEW.md
    try:
        with open("READMENEW1.md", "w", encoding="utf-8") as file:
            file.write(content)
        print("‚úÖ –§–∞–π–ª READMENEW1.md —É—Å–ø–µ—à–Ω–æ —Å–æ–∑–¥–∞–Ω!")
    except Exception as e:
        print(f"‚ùå –û—à–∏–±–∫–∞ –ø—Ä–∏ —Å–æ–∑–¥–∞–Ω–∏–∏ —Ñ–∞–π–ª–∞: {e}")

def parse_readme_new():
    """–§—É–Ω–∫—Ü–∏—è –¥–ª—è —á—Ç–µ–Ω–∏—è –∏ –ø–∞—Ä—Å–∏–Ω–≥–∞ —Ñ–∞–π–ª–∞ READMENEW1.md"""
    try:
        with open("READMENEW1.md", "r", encoding="utf-8") as file:
            content = file.read()
        
        # –ò—Å–ø–æ–ª—å–∑—É–µ–º —Ä–µ–≥—É–ª—è—Ä–Ω—ã–µ –≤—ã—Ä–∞–∂–µ–Ω–∏—è –¥–ª—è –∏–∑–≤–ª–µ—á–µ–Ω–∏—è –¥–∞–Ω–Ω—ã—Ö
        college_match = re.search(r'–£—á–µ–±–Ω–æ–µ –∑–∞–≤–µ–¥–µ–Ω–∏–µ:\s*(.+)', content)
        course_match = re.search(r'–ö—É—Ä—Å:\s*(.+)', content)
        name_match = re.search(r'–§–ò–û:\s*(.+)', content)
        group_match = re.search(r'–ö–æ–º–∞–Ω–¥–∞:\s*(.+)', content)
        id_match = re.search(r'ID:\s*(.+)', content)
        
        # –ò–∑–≤–ª–µ–∫–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è –∏–ª–∏ –∏—Å–ø–æ–ª—å–∑—É–µ–º –∑–Ω–∞—á–µ–Ω–∏—è –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
        college = college_match.group(1).strip() if college_match else "–ù–µ —É–∫–∞–∑–∞–Ω–æ"
        course = course_match.group(1).strip() if course_match else "–ù–µ —É–∫–∞–∑–∞–Ω–æ"
        name = name_match.group(1).strip() if name_match else "–ù–µ —É–∫–∞–∑–∞–Ω–æ"
        group = group_match.group(1).strip() if group_match else "–ù–µ —É–∫–∞–∑–∞–Ω–æ"
        id = id_match.group(1).strip() if id_match else "–ù–µ —É–∫–∞–∑–∞–Ω–æ"
        
        return college, course, name, group, id
        
    except FileNotFoundError:
        print("‚ùå –§–∞–π–ª READMENEW1.md –Ω–µ –Ω–∞–π–¥–µ–Ω!")
        return "–ù–µ —É–∫–∞–∑–∞–Ω–æ", "–ù–µ —É–∫–∞–∑–∞–Ω–æ", "–ù–µ —É–∫–∞–∑–∞–Ω–æ", "–ù–µ —É–∫–∞–∑–∞–Ω–æ", "–ù–µ —É–∫–∞–∑–∞–Ω–æ"
    except Exception as e:
        print(f"‚ùå –û—à–∏–±–∫–∞ –ø—Ä–∏ —á—Ç–µ–Ω–∏–∏ —Ñ–∞–π–ª–∞: {e}")
        return "–ù–µ —É–∫–∞–∑–∞–Ω–æ", "–ù–µ —É–∫–∞–∑–∞–Ω–æ", "–ù–µ —É–∫–∞–∑–∞–Ω–æ", "–ù–µ —É–∫–∞–∑–∞–Ω–æ", "–ù–µ —É–∫–∞–∑–∞–Ω–æ"

def output(profile):
    """–§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤—ã–≤–æ–¥–∞ –¥–∞–Ω–Ω—ã—Ö –∏–∑ README —Ñ–∞–π–ª–æ–≤"""
    def output_in():
        print("‚úÖ –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è —É—Å–ø–µ—à–Ω–æ –ø–æ–ª—É—á–µ–Ω–∞!")
        print()
        
        # –í—ã–≤–æ–¥–∏–º –ø—Ä–∏–≤–µ—Ç—Å—Ç–≤–∏–µ 5 —Ä–∞–∑
        for i in range(5):
            print("=" * 50)
            print(f"–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π—Ç–µ, {name}!") 
            print(f"–ü—Ä–∏–≤–µ—Ç—Å—Ç–≤—É–µ–º —Å—Ç—É–¥–µ–Ω—Ç–∞ {college}")
            print(f"–ö—É—Ä—Å: {course}")
            print(f"–ö–æ–º–∞–Ω–¥–∞: {group}")
            print(f"ID {id}")
            print("=" * 50)
            print()
        
        print("–ñ–µ–ª–∞–µ–º —É—Å–ø–µ—Ö–æ–≤ –≤ –æ–±—É—á–µ–Ω–∏–∏! üöÄ")
    
    # –ü–æ–ª—É—á–∞–µ–º –¥–∞–Ω–Ω—ã–µ –∏–∑ —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤—É—é—â–µ–≥–æ –∏—Å—Ç–æ—á–Ω–∏–∫–∞
    if profile == "1":
        college, course, name, group, id = parse_readme_001()
        output_in()
    elif profile == "2":
        college, course, name, group, id = parse_readme_002()
        output_in()
    elif profile == "3":
        college, course, name, group, id = parse_readme_003()  
        output_in()
    elif profile == "4":
        college, course, name, group, id = parse_readme_004()
        output_in()
    elif profile == "new":
        college, course, name, group, id = parse_readme_new()
        output_in()
    else:
        print("‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π –≤–≤–æ–¥! –í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ –æ—Ç 1 –¥–æ 4 –∏–ª–∏ 'new'")

if __name__ == "__main__":
    main()
